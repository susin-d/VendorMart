{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "routes": [
    {
      "src": "/api/vendors/register",
      "dest": "/api/vendors.js"
    },
    {
      "src": "/api/vendors/([^/]+)",
      "dest": "/api/vendors.js"
    },
    {
      "src": "/api/vendors/([^/]+)/inventory(.*)",
      "dest": "/api/inventory.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}